                                    <input class="form-control" :class="{ 'input-disabled' : inputDisable}" name="quantity" v-model="quantity" type="number">
                                </div>
                                <span v-if="!quantity_visibility">
                        <input  name="quantity" v-model="quantity" type="hidden">
                        </span>
